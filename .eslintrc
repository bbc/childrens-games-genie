{
    "env": { "es6": true },
    "extends": [
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2017
    },
    "plugins": [
        "no-only-tests"
    ],
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-undef": [0],
        "no-console": ["warn"],
        "no-only-tests/no-only-tests": [2]
    }
}
